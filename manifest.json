{
  "manifest_version": 3,
  "name": "Quick Linker",
  "version": "2.1.0",
  "description": "Quickly navigate to regularly accessed websites using keyword shortcuts.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  },
  "permissions": ["storage", "tabs"],
  "host_permissions": ["<all_urls>"],
  "omnibox": {
    "keyword": "gl"
  },
  "minimum_chrome_version": "102",
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "dependencies/storage/localStorageManager.js",
        "dependencies/storage/quickLinkManager.js"
      ]
    }
  ],
  "options_page": "options/options.html",
  "web_accessible_resources": [
    {
      "resources": ["search/search.html", "search/search.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
